<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="OnRibbonLoad">
  <ribbon>
    <tabs>
      <tab idMso="TabDeveloper">
        <group id="Group1" label="VBA Import Export">

          <button id="ExportVBA" size="large" getLabel="OnGetRibbonLabel" onAction="OnRibbonButtonClick" getEnabled="IsRibbonCommandEnabled" getImage="GetRibbonImage" />
          <button id="ImportVBA" size="large" getLabel="OnGetRibbonLabel" onAction="OnRibbonButtonClick" getEnabled="IsRibbonCommandEnabled" getImage="GetRibbonImage" />

          <separator id="s1" />

          <checkBox id="IncludeStencils" getLabel="OnGetRibbonLabel" onAction="OnRibbonButtonCheckClick" getEnabled="IsRibbonCommandEnabled" getPressed="IsRibbonCommandChecked" />
          <checkBox id="ClearBeforeImport" getLabel="OnGetRibbonLabel" onAction="OnRibbonButtonCheckClick" getEnabled="IsRibbonCommandEnabled" getPressed="IsRibbonCommandChecked" />
                    
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>
